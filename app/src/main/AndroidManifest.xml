<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hydbest.behavior">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.hydbest.activity.SampleTitleActivity"></activity>
        <activity android:name="com.hydbest.activity.ScrollActivity"></activity>
        <activity android:name="com.hydbest.activity.UCActivity"/>
        <activity android:name="com.hydbest.activity.AvatarScollActivity"></activity>
        <activity android:name="com.hydbest.activity.BottomSheetActivity"></activity>
        <activity android:name="com.hydbest.activity.SwipeDismissActivity"></activity>
        <activity android:name="com.hydbest.activity.CoverHeaderActivity"></activity>
        <activity android:name="com.hydbest.activity.NdkActivity"></activity>
        <activity android:name="com.hydbest.activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>